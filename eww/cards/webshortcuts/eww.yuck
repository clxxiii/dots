
(defwindow youtube 
  :monitor 0
  :geometry (geometry :x "${320 + 167 * 6}px"
                      :y "${140 + 73 * 6}px"
                      :anchor "top left")
  :stacking "bg"
  :focusable false
  :hexpand false
  (webshortcut :icon "󰗃" :link "https://youtube.com" :color "#FF0000")
  )

